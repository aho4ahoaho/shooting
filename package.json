{
    "name": "shooting",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host 0.0.0.0",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "format": "prettier --write .",
        "prepare": "husky install"
    },
    "dependencies": {
        "preact": "^10.19.1"
    },
    "devDependencies": {
        "@preact/preset-vite": "^2.6.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "typescript": "^5.2.2",
        "typescript-eslint": "^7.0.1",
        "vite": "^5.0.0"
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,css,scss}": [
            "prettier --write"
        ]
    }
}
