{
    "name": "shooting",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host 0.0.0.0",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "format": "prettier --write .",
        "prepare": "husky"
    },
    "dependencies": {
        "preact": "^10.24.2",
        "sass": "^1.79.5"
    },
    "devDependencies": {
        "@preact/preset-vite": "^2.9.1",
        "eslint": "^8",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.1",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.8.1",
        "vite": "^5.4.8"
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "eslint --fix",
            "prettier --write"
